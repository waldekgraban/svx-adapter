{
    "name": "waldekgraban/svx-adapter",
    "description":"SVX file adapter to PHP objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Waldemar Graban",
            "email": "waldekgraban@gmail.com"
        },
        {
            "name": "Krzysztof Grabania",
            "email": "krzysztof@grabania.pl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/PHP-Survex/PHP-Survex.git"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Waldekgraban\\SvxAdapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waldekgraban\\SvxAdapter\\": "src/",
            "Waldekgraban\\SvxAdapter\\Tests\\": "tests/"
        }
    }
}
